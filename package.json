{
  "name": "taro3-react-template",
  "version": "0.0.1",
  "private": true,
  "description": "Taro3 React Template",
  "templateInfo": {
    "name": "taro-hooks-mobx",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp-dev": "cross-env BUILD_ENV=dev taro build --type weapp",
    "build:weapp-prod": "cross-env BUILD_ENV=prod taro build --type weapp",
    "dev:weapp-dev": "cross-env BUILD_ENV=dev yarn run build:weapp-dev -- --watch",
    "dev:weapp-prod": "cross-env BUILD_ENV=prod yarn run build:weapp-prod -- --watch",
    "build:alipay-dev": "cross-env BUILD_ENV=dev taro build --type alipay",
    "build:alipay": "cross-env BUILD_ENV=prod taro build --type alipay",
    "dev:alipay-dev": "cross-env BUILD_ENV=dev yarn run build:alipay -- --watch",
    "dev:alipay": "cross-env BUILD_ENV=prod yarn run build:alipay -- --watch",
    "build:h5-dev": "cross-env BUILD_ENV=dev taro build --type h5",
    "build:h5": "cross-env BUILD_ENV=prod taro build --type h5",
    "dev:h5-dev": "cross-env BUILD_ENV=dev yarn run build:h5 -- --watch",
    "dev:h5": "cross-env BUILD_ENV=prod yarn run build:h5 -- --watch",
    "build:swan": "taro build --type swan",
    "build:tt": "taro build --type tt",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.js": "eslint --ext .js",
    "src/**/*.ts": "eslint --ext .ts",
    "src/**/*.tsx": "eslint --ext .tsx"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.4",
    "ios >= 9"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/cli": "3.4.0",
    "@tarojs/components": "3.4.0",
    "@tarojs/plugin-framework-react": "3.4.0",
    "@tarojs/react": "3.4.0",
    "@tarojs/runtime": "3.4.0",
    "@tarojs/taro": "3.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "taro-hooks": "latest"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@tarojs/mini-runner": "3.4.0",
    "@tarojs/webpack-runner": "3.4.0",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-plugin-import": "^1.13.3",
    "babel-preset-taro": "3.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-config-taro": "3.4.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "mini-types": "*",
    "miniprogram-api-typings": "*",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "stylelint": "9.3.0",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}